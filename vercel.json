{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      },
      {
        "handle": "options",
        "src": "/(.*)",
        "status": 204
      }
    ],
    "middlewares": [
      {
        "function": "vercel-cors"
      }
    ]
  }
  